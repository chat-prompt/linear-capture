{
  "name": "linear-capture",
  "version": "1.0.0",
  "description": "Capture screen and create Linear issues instantly",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "npm run build && unset ELECTRON_RUN_AS_NODE && electron .",
    "dev": "tsc && npm run copy-assets && unset ELECTRON_RUN_AS_NODE && electron .",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/renderer && cp src/renderer/*.html dist/renderer/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "linear",
    "screenshot",
    "capture",
    "macos"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "electron": "^28.3.3",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@linear/sdk": "^38.0.0",
    "dotenv": "^16.3.1",
    "electron-store": "^8.1.0"
  }
}
